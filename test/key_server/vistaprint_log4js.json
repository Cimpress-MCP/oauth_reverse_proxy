{
   "appenders": [
   {
      "type": "logLevelFilter",
      "levels": "trace",
      "level": "info",
      "appender": {
         "type": "file",
         "filename": "logs/key_server.log",
         "maxLogSize": 20485760,
         "backups": 50,
         "pollInterval": 15
      }
   },{
      "type": "logLevelFilter",
      "levels": "debug",
      "level": "trace",
      "appender": {
         "type": "console"
      }
   }]
}
